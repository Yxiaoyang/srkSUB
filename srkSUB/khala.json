{
   "types": {
      "Address": "MultiAddress",
      "LookupSource": "MultiAddress",
      "Keys": "AccountId",
      "ChainId": "u8",
      "BridgeChainId": "u8",
      "BridgeEvent": {
        "type": "enum",
        "value_list": [
            ["FungibleTransfer", "FungibleTransfer"],
            ["NonFungibleTransfer", "NonFungibleTransfer"],
            ["GenericTransfer", "GenericTransfer"]
        ]
    },
    "FungibleTransfer": {
      "type": "struct",
      "type_mapping": [
        ["destId", "BridgeChainId"],
        ["nonce", "DepositNonce"],
        ["resourceId", "ResourceId"],
        ["amount", "U256"],
        ["recipient", "Vec<u8>"]
      ]
    },
    "NonFungibleTransfer": {
      "type": "struct",
      "type_mapping": [
        ["destId", "BridgeChainId"],
        ["nonce", "DepositNonce"],
        ["resourceId", "ResourceId"],
        ["tokenId", "Vec<u8>"],
        ["recipient", "Vec<u8>"],
        ["metadata", "Vec<u8>"]
      ]
    },
    "GenericTransfer": {
      "type": "struct",
      "type_mapping": [
        ["destId", "BridgeChainId"],
        ["nonce", "DepositNonce"],
        ["resourceId", "ResourceId"],
        ["metadata", "Vec<u8>"]
      ]
    },
    "ResourceId": "[u8; 32]",
    "TokenId": "U256",
    "DepositNonce": "u64",
    "ProposalStatus": {
      "type": "enum",
      "value_list": [
        ["Initiated", null],
        ["Approved", null],
        ["Rejected", null]
      ]
    },
    "ProposalVotes": {
      "type": "struct",
      "type_mapping": [
        ["votesFor", "Vec<AccountId>"],
        ["votesAgainst", "Vec<AccountId>"],
        ["status", "ProposalStatus"],
        ["expiry", "BlockNumber"]
      ]
    },
    "AssetInfo": {
      "type": "struct",
      "type_mapping": [
        ["destId", "BridgeChainId"],
        ["assetIdentity", "Vec<u8>"]
      ]
    },
    "ProxyType": {
      "type": "enum",
      "value_list": [
        "Any",
        "NonTransfer",
        "CancelProxy",
        "Governance",
        "Collator",
        "StakePoolManager"
      ]
    },
    "Sr25519PublicKey": "[u8; 32]",
    "MasterPublicKey": "Sr25519PublicKey",
    "WorkerPublicKey": "Sr25519PublicKey",
    "ContractPublicKey": "Sr25519PublicKey",
    "EcdhPublicKey": "[u8; 32]",
    "MessageOrigin": {
      "type": "enum",
      "value_list": [
        ["Pallet", "Vec<u8>"],
        ["Contract", "H256"],
        ["Worker", "Sr25519PublicKey"],
        ["AccountId", "H256"],
        ["MultiLocation", "Vec<u8>"],
        ["Gatekeeper", null]
      ]
    },
    "Attestation": {
      "type": "enum",
      "value_list": [
        ["SgxIas", "AttestationSgxIas"]
      ]
    },
    "AttestationSgxIas": {
      "type": "struct",
      "type_mapping": [
        ["raReport", "Vec<u8>"],
        ["signature", "Vec<u8>"],
        ["rawSigningCert", "Vec<u8>"]
      ]
    },
    "SenderId": "MessageOrigin",
    "Path": "Vec<u8>",
    "Topic": "Path",
    "Message": {
      "type": "struct",
      "type_mapping": [
        ["sender", "SenderId"],
        ["destination", "Topic"],
        ["payload", "Vec<u8>"]
      ]
    },
    "SignedMessage": {
      "type": "struct",
      "type_mapping": [
        ["message", "Message"],
        ["sequence", "u64"],
        ["signature", "Vec<u8>"]
      ]
    },
    "WorkerRegistrationInfo": {
      "type": "struct",
      "type_mapping": [
        ["version", "u32"],
        ["machineId", "Vec<u8>"],
        ["pubkey", "WorkerPublicKey"],
        ["ecdhPubkey", "EcdhPublicKey"],
        ["genesisBlockHash", "H256"],
        ["features", "Vec<u32>"],
        ["operator", "Option<AccountId>"]
      ]
    },
    "PoolInfo": {
      "type": "struct",
      "type_mapping": [
        ["pid", "u64"],
        ["owner", "AccountId"],
        ["payoutCommission", "Option<Permill>"],
        ["ownerReward", "Balance"],
        ["cap", "Option<Balance>"],
        ["rewardAcc", "u128"],
        ["totalShares", "Balance"],
        ["totalStake", "Balance"],
        ["freeStake", "Balance"],
        ["releasingStake", "Balance"],
        ["workers", "Vec<WorkerPublicKey>"],
        ["withdrawQueue", "Vec<WithdrawInfo>"]
      ]
    },
    "WithdrawInfo": {
      "type": "struct",
      "type_mapping": [
        ["user", "AccountId"],
        ["shares", "Balance"],
        ["startTime", "u64"]
      ]
    },
    "WorkerInfo": {
      "type": "struct",
      "type_mapping": [
        ["pubkey", "WorkerPublicKey"],
        ["ecdhPubkey", "EcdhPublicKey"],
        ["runtimeVersion", "u32"],
        ["lastUpdated", "u64"],
        ["operator", "Option<AccountId>"],
        ["confidenceLevel", "u8"],
        ["initialScore", "Option<u32>"],
        ["features", "Vec<u32>"]
      ]
    },
    "MinerInfo": {
      "type": "struct",
      "type_mapping": [
        ["state", "MinerState"],
        ["ve", "u128"],
        ["v", "u128"],
        ["vUpdatedAt", "u64"],
        ["benchmark", "Benchmark"],
        ["coolDownStart", "u64"],
        ["stats", "MinerStats"]
      ]
    },
    "Benchmark": {
      "type": "struct",
      "type_mapping": [
        ["pInit", "u32"],
        ["pInstant", "u32"],
        ["iterations", "u64"],
        ["miningStartTime", "u64"],
        ["challengeTimeLast", "u64"]
      ]
    },
    "MinerState": {
      "type": "enum",
      "value_list": [
        ["Ready", null],
        ["MiningIdle", null],
        ["MiningActive", null],
        ["MiningUnresponsive", null],
        ["MiningCoolingDown", null]
      ]
    },
    "MinerStats": {
      "type": "struct",
      "type_mapping": [
        ["totalReward", "Balance"]
      ]
    },
    "HeartbeatChallenge": {
      "type": "struct",
      "type_mapping": [
        ["seed", "U256"],
        ["onlineTarget", "U256"]
      ]
    },
    "KeyDistribution": {
      "type": "enum",
      "value_list": [
        ["MasterKeyDistribution", "DispatchMasterKeyEvent"]
      ]
    },
    "GatekeeperLaunch": {
      "type": "enum",
      "value_list": [
        ["FirstGatekeeper", "NewGatekeeperEvent"],
        ["MasterPubkeyOnChain", null]
      ]
    },
    "GatekeeperChange": {
      "type": "enum",
      "value_list": [
        ["GatekeeperRegistered", "NewGatekeeperEvent"]
      ]
    },
    "GatekeeperEvent": {
      "type": "enum",
      "value_list": [
        ["NewRandomNumber", "RandomNumberEvent"],
        ["TokenomicParametersChanged", "TokenomicParameters"]
      ]
    },
    "NewGatekeeperEvent": {
      "type": "struct",
      "type_mapping": [
        ["pubkey", "WorkerPublicKey"],
        ["ecdhPubkey", "EcdhPublicKey"]
      ]
    },
    "DispatchMasterKeyEvent": {
      "type": "struct",
      "type_mapping": [
        ["dest", "WorkerPublicKey"],
        ["ecdhPubkey", "EcdhPublicKey"],
        ["encryptedMasterKey", "Vec<u8>"],
        ["iv", "[u8; 12]"]
      ]
    },
    "RandomNumberEvent": {
      "type": "struct",
      "type_mapping": [
        ["blockNumber", "u32"],
        ["randomNumber", "[u8; 32]"],
        ["lastRandomNumber", "[u8; 32]"]
      ]
    },
    "TokenomicParameters": {
      "type": "struct",
      "type_mapping": [
        ["phaRate", "U64F64Bits"],
        ["rho", "U64F64Bits"],
        ["budgetPerBlock", "U64F64Bits"],
        ["vMax", "U64F64Bits"],
        ["costK", "U64F64Bits"],
        ["costB", "U64F64Bits"],
        ["slashRate", "U64F64Bits"],
        ["treasuryRatio", "U64F64Bits"],
        ["heartbeatWindow", "u32"],
        ["rigK", "U64F64Bits"],
        ["rigB", "U64F64Bits"],
        ["re", "U64F64Bits"],
        ["k", "U64F64Bits"],
        ["kappa", "U64F64Bits"]
      ]
    },
    "TokenomicParams": "TokenomicParameters",
    "U64F64Bits": "u128",
    "UserStakeInfo": {
      "type": "struct",
      "type_mapping": [
        ["user", "AccountId"],
        ["locked", "Balance"],
        ["shares", "Balance"],
        ["availableRewards", "Balance"],
        ["rewardDebt", "Balance"]
      ]
    }
  }
}